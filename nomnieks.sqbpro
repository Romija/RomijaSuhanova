idths><column index="1" value="76"/><column index="2" value="73"/><column index="3" value="118"/><column index="4" value="71"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">--parādīt visus nomniekus
SELECT*FROM nomnieks;

--parādīt visus produktus
SELECT*FROM produkts;

--parādīt tikai nomnieka vārdus un uzvārdus
SELECT vards, uzvards FROM nomnieks;

--parādīt visus produktus, kuru cena dienā ir lielāka par 15
SELECT nomas_cena_diena,nosaukums FROM produkts WHERE nomas_cena_diena&gt;15;

--parādīt pieejamos produktus(pieejamiba='Jā')
SELECT*FROM produkts WHERE pieejamiba=&quot;Jā&quot;;

--atrast visus produktus, ko īrē nomnieks ar id 2
SELECT*FROM noma WHERE nomnieks_id=2;

SELECT*FROM produkts JOIN noma ON produkts.produkts_id=noma.produkts_id
WHERE noma.nomnieks_id=2;

--parādīt visus nomas darījumus ar nomnieka un produkta info
SELECT noma.noma_id, nomnieks.vards, nomnieks.uzvards,produkts.nosaukums,
noma.sak_datums, noma.beigu_datums FROM noma 
JOIN nomnieks ON noma.nomnieks_id=nomnieks.nomnieks_id 
JOIN produkts ON noma.produkts_id=produkts.produkts_id;</sql><current_tab id="0"/></tab_sql></sqlb_project>
